package programa;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
			int [] vetor = new int[5];
			
			for (int i = 0; i < vetor.length; i++) {
				System.out.print("Escreva uma idade: ");
				int idade = sc.nextInt();
				vetor[i] = idade;
			}
			
			
			System.out.print("[");
			for (int i = 0; i < vetor.length; i++) {
				System.out.print(" "+vetor[i]+" ");
			}
			System.out.print("]");
			
		sc.close();
	}

}
