package listaEncadeada;

public class Livro {
	private String nome;
	private double preco;
	
	public Livro() {
		this.nome = null;
		this.preco = 0.0;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public double getPreco() {
		return preco;
	}

	public void setPreco(double preco) {
		this.preco = preco;
	}
	
	
}
