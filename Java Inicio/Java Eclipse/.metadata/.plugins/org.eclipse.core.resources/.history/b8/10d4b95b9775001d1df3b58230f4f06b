package listaEncadeada;

public class Celula {
	private Celula proxima; //como os dados s√£o armazenados//
	private Livro livro;
	
	public Celula(Celula proxima, Livro livro) {
		super();
		this.proxima = proxima;
		this.livro = livro;
	}

	public Celula getProxima() {
		return proxima;
	}

	public void setProxima(Celula proxima) {
		this.proxima = proxima;
	}

	public Livro getLivro() {
		return livro;
	}

	public void setLivro(Livro livro) {
		this.livro = livro;
	}
	
	
	
	
}
